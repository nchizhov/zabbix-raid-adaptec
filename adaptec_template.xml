                    <uuid>130b3e9b71ef430a8d9567e83c02d452</uuid>
                    <name>Adaptec Controller #{$ADAPTEC.CONTROLLER.INDEX} BBU Capacity</name>
                    <key>raid.adaptec.info[{$ADAPTEC.CONTROLLER.INDEX},Controller Battery Information.Capacity remaining]</key>
                    <delay>2m</delay>
                    <history>31d</history>
                    <units>%</units>
                    <description>Controller Battery Backup Unit Capacity remaining (if exists)</description>
                    <preprocessing>
                        <step>
                            <type>REGEX</type>
                            <parameters>
                                <parameter>^(\d+) percent$</parameter>
                                <parameter>\1</parameter>
                            </parameters>
                        </step>
                    </preprocessing>
                    <tags>
                        <tag>
                            <tag>Application</tag>
                            <value>RAID Controller</value>
                        </tag>
                    </tags>
                    <triggers>
                        <trigger>
                            <uuid>7a9be613c78b47e591451e5ace7b0b25</uuid>
                            <expression>last(/adaptec_raid/raid.adaptec.info[{$ADAPTEC.CONTROLLER.INDEX},Controller Battery Information.Capacity remaining])&lt;5</expression>
